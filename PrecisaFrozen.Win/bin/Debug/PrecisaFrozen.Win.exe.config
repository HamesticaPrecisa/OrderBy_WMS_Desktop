<?xml version="1.0"?>
<configuration>
    <configSections>
    </configSections>
  <system.windows.forms jitDebugging="true" />
  <startup  useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0"/>
  </startup>
    <appSettings>

      <!-- SET DE CONFIGURACION ACTUAL (dev, qas o prd) -->
      <add key="app.config.set" value="dev" />
      
      <!-- DEV CONFIG SET -->
      <add key="app.dev.db.ip.lan" value="localhost"/>
      <add key="app.dev.db.ip.wlan" value="localhost" />
      <add key="app.dev.db.uid" value="sa" />
      <add key="app.dev.db.pwd" value="precisa" />
      <add key="app.dev.db.catalogs.main" value="precisa_backup" />      
      <add key="app.dev.db.catalogs.etiquetado"  value="etiquetado" />

      <!-- QAS CONFIG SET -->
      <add key="app.qas.db.ip.lan" value="192.168.1.90\PRECISABD"/>
      <add key="app.qas.db.ip.wlan" value="192.168.1.90\PRECISABD" />
      <add key="app.qas.db.uid" value="sa" />
      <add key="app.qas.db.pwd" value="precisa" />     
      <add key="app.qas.db.catalogs.main" value="precisa_backup" />
      <add key="app.qas.db.catalogs.etiquetado"  value="etiquetado" />

      <!-- PRD CONFIG SET -->
      <add key="app.prd.db.ip.lan" value="192.168.1.90\PRECISABD"/>
      <add key="app.prd.db.ip.wlan" value="192.168.1.90\PRECISABD" />
      <add key="app.prd.db.uid" value="sa" />
      <add key="app.prd.db.pwd" value="precisa" />      
      <add key="app.prd.db.catalogs.main" value="precisa" />
      <add key="app.prd.db.catalogs.etiquetado"  value="etiquetado" />      

    </appSettings>
    <connectionStrings>
      <add name="PrecisaFrozen.Win.My.MySettings.Ds_General" connectionString="Dsn=Precisa_Informes;uid=sa" providerName="System.Data.Odbc"/>
      <add name="PrecisaFrozen.Win.My.MySettings.GuiaRecepcion" connectionString="Dsn=Informes;uid=sa;app=Microsoft® Visual Studio® 2010;wsid=PROGRAMADOR-PC;database=PRECISA_BD" providerName="System.Data.Odbc"/>
      <add name="app.connstrings.lan" connectionString="server={ip};uid={uid};pwd={pwd};Connection Timeout=100;" providerName="System.Data.Odbc" />
      <add name ="app.connstrings.wlan" connectionString="server={ip};uid={uid};pwd={pwd};Connection Timeout=100;" providerName="System.Data.Odbc" />
    </connectionStrings>
    <system.diagnostics>
        <sources>
            <!-- This section defines the logging configuration for My.Application.Log -->
            <source name="DefaultSource" switchName="DefaultSwitch">
                <listeners>
                    <add name="FileLog"/>
                    <!-- Uncomment the below section to write to the Application Event Log -->
                    <!--<add name="EventLog"/>-->
                </listeners>
            </source>
        </sources>
        <switches>
            <add name="DefaultSwitch" value="Information"/>
        </switches>
        <sharedListeners>
            <add name="FileLog" type="Microsoft.VisualBasic.Logging.FileLogTraceListener, Microsoft.VisualBasic, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a,processorArchitecture=MSIL" initializeData="FileLogWriter"/>
            <!-- Uncomment the below section and replace APPLICATION_NAME with the name of your application to write to the Application Event Log -->
            <!--<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="APPLICATION_NAME"/> -->
        </sharedListeners>
    </system.diagnostics>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
